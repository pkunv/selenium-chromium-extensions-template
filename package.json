{
	"name": "selenium-chromium-extensions-template",
	"version": "0.1.0",
	"description": "Selenium template to run headless chromium with extensions",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"build:worker": "esbuild ./src/worker.ts --bundle --minify --platform=node --format=esm --main-fields=module,main --outfile=dist/worker.js --packages=external",
		"start": "esbuild ./src/index.ts --bundle --minify --platform=node --format=esm --main-fields=module,main --outfile=dist/index.js --packages=external && node dist/index.js"
	},
	"author": "pkunv",
	"license": "ISC",
	"devDependencies": {
		"@eslint/js": "^9.11.1",
		"@types/selenium-webdriver": "^4.1.26",
		"esbuild": "^0.24.0",
		"eslint": "^9.11.1",
		"globals": "^15.9.0",
		"prettier": "^3.3.3",
		"tsx": "^4.19.1",
		"typescript": "^5.6.2",
		"typescript-eslint": "^8.7.0"
	},
	"dependencies": {
		"dotenv": "^16.4.5",
		"selenium-webdriver": "^4.25.0"
	}
}
